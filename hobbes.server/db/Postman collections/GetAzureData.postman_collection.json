	"item": [
		{
			"name": "GetAzurePlaceholder",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "03bed438-621b-4d53-8433-93d5e4d3d754",
						"exec": [
							"var data = pm.response.json();",
							"Date.prototype.yyyymmdd = function() {",
							"  var mm = this.getMonth() + 1; // getMonth() is zero-based",
							"  var dd = this.getDate();",
							"",
							"  return [this.getFullYear(),",
							"          (mm>9 ? '' : '0') + mm,",
							"          (dd>9 ? '' : '0') + dd",
							"         ].join('-');",
							"};",
							"pm.environment.set(\"doc\", JSON.stringify( {",
							"    \"_id\" : request.url.toString(),",
							"    \"project\" : pm.environment.get('AzureProject'),",
							"    \"source\" : \"Azure DevOps\",",
							"    \"timestamp\" : new Date().yyyymmdd(),",
							"    \"data\" : data",
							"}));"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "435cf740-9b02-4a91-bb08-84b21c117852",
						"exec": [
							"var request = pm.request;",
							"console.log(\"url\",request.url);",
							"pm.environment.set(\"skiptoken\", \"\");",
							"pm.environment.set(\"data\",\"\");",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "4m64rdzd3cohe2c463uzdozttn6i4yh6cbqfpni26acbls75jzva",
							"type": "string"
						},
						{
							"key": "username",
							"value": "4m64rdzd3cohe2c463uzdozttn6i4yh6cbqfpni26acbls75jzva",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/x-www-form-urlencoded",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
