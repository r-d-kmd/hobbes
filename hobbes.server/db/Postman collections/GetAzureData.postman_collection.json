		{
			"name": "PostToRawData",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "8fabbc26-8aa0-4b26-8ead-1fb7c4cd8ebc",
						"exec": [
							"console.log(\"doc\",JSON.parse(pm.environment.get(\"doc\")));",
							"console.log(pm.request.body);",
							"pm.environment.set(\"doc\",\"{}\");"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "6f6d8a05-d3a6-4b60-bbce-c7cd01a1fad0",
						"exec": [
							"var doc = JSON.parse(pm.environment.get(\"doc\"));",
							"",
							"console.log(\"data\", doc, doc.data);",
							"",
							"if(doc.data.hasOwnProperty(\"@odata.nextLink\")) {",
							"    var parts = doc.data[\"@odata.nextLink\"].split(\"skiptoken=\");",
							"    var skipToken = parts[parts.length - 1];",
							"    pm.environment.set(\"skipToken\", skipToken);",
							"    postman.setNextRequest('GetAzurePlaceholder'); ",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "password",
							"type": "string"
						},
						{
							"key": "username",
							"value": "admin",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{{doc}}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{front}}/db/rawdata/",
					"host": [
						"{{front}}"
					],
					"path": [
						"db",
						"rawdata",
						""
					]
				}
			},
			"response": []
		},
